<canvas id="pie_graph"></canvas>
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
<script>
var ctx = document.getElementById('pie_graph');
var myChart = new Chart(ctx, {
    type: 'pie',
    data: {
        labels: ['1', '2', '3', '4'],
        datasets: [{
            backgroundColor: [
              "#c97586",
              "#bbbcde",
              "#93b881",
              "#e6b422"
            ],
            data: <%= ranks_array %>
        }]
    },
    options: {

    }
});
</script>